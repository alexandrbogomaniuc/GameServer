import GUGameBaseDialogInfo from './GUGameBaseDialogInfo';

class GUGamePendingOperationFailedDialogInfo extends GUGameBaseDialogInfo
{
	constructor(dialogId, priority)
	{
		super(dialogId, priority);

		this._retryType = null;
	}

	get isActivationOverHiddenGameAvailable()
	{
		return true;
	}

	set retryType(value)
	{
		this._retryType = value;
	}

	get retryType()
	{
		return this._retryType;
	}
}

export default GUGamePendingOperationFailedDialogInfo