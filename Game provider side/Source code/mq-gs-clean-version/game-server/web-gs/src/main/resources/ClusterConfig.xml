            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.cassandra.persist.mp.RoundKPIInfoPersister</className>
            <!-- 1 day -->
            <ttl>86400</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.cassandra.persist.mp.BattlegroundHistoryPersister</className>
            <!-- 120 days -->
            <ttl>10368000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.promo.persisters.PromoPredefinedUsersPersister</className>
            <!-- 180 days -->
            <ttl>15552000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.cassandra.persist.CassandraClientStatisticsPersister</className>
            <!-- 3 days -->
            <ttl>259200</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.cassandra.persist.mp.BattlegroundPrivateRoomSettingsPersister</className>
            <!-- 120 days -->
            <ttl>10368000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.promo.persisters.CassandraPromoWinPersister</className>
            <!-- 30 days -->
            <ttl>2592000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.dgphoenix.casino.cassandra.persist.CassandraBatchOperationStatusPersister</className>
            <!-- 3 days -->
            <ttl>259200</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
    </columnFamilies>
</ClusterConfig>