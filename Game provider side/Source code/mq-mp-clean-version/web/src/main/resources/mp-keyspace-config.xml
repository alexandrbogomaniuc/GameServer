<ClusterConfig>
    <clusterName>MPCluster</clusterName>
    <keySpace>${cassandra.metadata.keyspaceName}</keySpace>
    <hostTimeoutWindow>30000</hostTimeoutWindow>
    <replicationStrategyClass>${cassandra.replicationStrategyClass}</replicationStrategyClass>
    <readConsistencyLevel>${cassandra.readConsistencyLevel}</readConsistencyLevel>
    <writeConsistencyLevel>${cassandra.writeConsistencyLevel}</writeConsistencyLevel>
    <replicationFactor>${cassandra.replicationFactor}</replicationFactor>
    <createScheme>${cassandra.createScheme}</createScheme>
    <hosts>c1:9042</hosts>
    <minimumOnlineHosts>${cassandra.general.minimumOnlineHosts}</minimumOnlineHosts>
    <columnFamilies>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.LockPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.SequencerPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.RoomPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.ServerConfigPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.RoomTemplatePersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.PlayerStatsPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.PlayerProfilePersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.PlayerNicknamePersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.WeaponsPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.ActiveFrbSessionPersister</className>
            <!-- 120 days -->
            <ttl>10368000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.ActiveCashBonusSessionPersister</className>
            <!-- 120 days -->
            <ttl>10368000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.TournamentSessionPersister</className>
            <!-- 120 days -->
            <ttl>10368000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.RoundResultNotificationPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.SingleNodeRoomInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.BGPrivateRoomInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.MultiNodePrivateRoomInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.MultiNodeRoomInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.BGPrivateRoomPlayersStatusPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.FriendsPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.OnlinePlayerPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.SocketClientInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.SocketClientCountPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.GameRoomSnapshotPersister</className>
            <!-- 10 days -->
            <ttl>864000</ttl>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.RoomPlayerInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.PlayerQuestsPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.GameConfigPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.ReservedNicknamePersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.MapConfigPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.SpawnConfigPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.MultiNodeSeatPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.BotConfigInfoPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.BotServiceConfigPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>

        <ColumnFamily>
            <className>com.betsoft.casino.mp.data.persister.PendingOperationPersister</className>
            <enabled>true</enabled>
        </ColumnFamily>
    </columnFamilies>
</ClusterConfig>
